    "class Fraction:\n",
    "    def __init__(self, x, y):\n",
    "        self.numerator = x\n",
    "        self.denominator = y\n",
    "\n",
    "    def __str__(self):\n",
    "        return '{}/{}'.format(self.numerator, self.denominator)\n",
    "\n",
